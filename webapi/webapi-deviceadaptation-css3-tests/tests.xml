<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="./testcase.xsl"?>
<test_definition>
  <suite category="W3C/HTML5 APIs" launcher="xwalk" name="webapi-deviceadaptation-css3-tests">
    <set name="CSSDeviceAdaptation">
      <testcase component="WebAPI/Responsive Design/CSS Device Adaptation" execution_type="manual" id="orientation_landscape" purpose="Test checks that the document should be locked to landscape presentation">
        <description>
          <test_script_entry>/opt/webapi-deviceadaptation-css3-tests/cssdeviceadaptation/orientation_landscape.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="WebAPI/Responsive Design/CSS Device Adaptation" execution_type="manual" id="orientation_portrait" purpose="Test checks that the document should be locked to portrait presentation">
        <description>
          <test_script_entry>/opt/webapi-deviceadaptation-css3-tests/cssdeviceadaptation/orientation_portrait.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="WebAPI/Responsive Design/CSS Device Adaptation" execution_type="manual" id="user-zoom_fixed" purpose="Test checks that the user cannot interactively change the zoom factor">
        <description>
          <test_script_entry>/opt/webapi-deviceadaptation-css3-tests/cssdeviceadaptation/user-zoom_fixed.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="WebAPI/Responsive Design/CSS Device Adaptation" execution_type="manual" id="zoom_0_point_5" purpose="Test checks that zoom factor gives a zoomed-out effect when the value of zoom is 0.5">
        <description>
          <test_script_entry>/opt/webapi-deviceadaptation-css3-tests/cssdeviceadaptation/zoom_0_point_5.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="WebAPI/Responsive Design/CSS Device Adaptation" execution_type="manual" id="zoom_1_point_0" purpose="Test checks that no zooming is done when the value of zoom is 1.0">
        <description>
          <test_script_entry>/opt/webapi-deviceadaptation-css3-tests/cssdeviceadaptation/zoom_1_point_0.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="WebAPI/Responsive Design/CSS Device Adaptation" execution_type="manual" id="zoom_2_point_0" purpose="Test checks that zoom factor gives a zoomed-in effect when the value of zoom is 2.0">
        <description>
          <test_script_entry>/opt/webapi-deviceadaptation-css3-tests/cssdeviceadaptation/zoom_2_point_0.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="WebAPI/Responsive Design/CSS Device Adaptation" execution_type="manual" id="zoom_50_percentage" purpose="Test checks that zoom factor gives a zoomed-out effect when the value of zoom is 50 percentage">
        <description>
          <test_script_entry>/opt/webapi-deviceadaptation-css3-tests/cssdeviceadaptation/zoom_50_percentage.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="WebAPI/Responsive Design/CSS Device Adaptation" execution_type="manual" id="zoom_100_percentage" purpose="Test checks that no zooming is done when the value of zoom is 100 percentage">
        <description>
          <test_script_entry>/opt/webapi-deviceadaptation-css3-tests/cssdeviceadaptation/zoom_100_percentage.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="WebAPI/Responsive Design/CSS Device Adaptation" execution_type="manual" id="zoom_200_percentage" purpose="Test checks that zoom factor gives a zoomed-in effect when the value of zoom is 200 percentage">
        <description>
          <test_script_entry>/opt/webapi-deviceadaptation-css3-tests/cssdeviceadaptation/zoom_200_percentage.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="WebAPI/Responsive Design/CSS Device Adaptation" execution_type="manual" id="zoom_auto" purpose="Test checks that the init value of zoom is auto">
        <description>
          <test_script_entry>/opt/webapi-deviceadaptation-css3-tests/cssdeviceadaptation/zoom_auto.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="WebAPI/Responsive Design/CSS Device Adaptation" execution_type="manual" id="max-zoom_auto" purpose="Test checks that the value of max-zoom is auto">
        <description>
          <test_script_entry>/opt/webapi-deviceadaptation-css3-tests/cssdeviceadaptation/max-zoom_auto.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="WebAPI/Responsive Design/CSS Device Adaptation" execution_type="manual" id="max-zoom_number" purpose="Test checks that the value of max-zoom is number">
        <description>
          <test_script_entry>/opt/webapi-deviceadaptation-css3-tests/cssdeviceadaptation/max-zoom_number.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="WebAPI/Responsive Design/CSS Device Adaptation" execution_type="manual" id="max-zoom_percentage" purpose="Test checks that the value of max-zoom is percentage">
        <description>
          <test_script_entry>/opt/webapi-deviceadaptation-css3-tests/cssdeviceadaptation/max-zoom_percentage.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="WebAPI/Responsive Design/CSS Device Adaptation" execution_type="manual" id="min-zoom_auto" purpose="Test checks that the value of min-zoom is auto">
        <description>
          <test_script_entry>/opt/webapi-deviceadaptation-css3-tests/cssdeviceadaptation/min-zoom_auto.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="WebAPI/Responsive Design/CSS Device Adaptation" execution_type="manual" id="min-zoom_number" purpose="Test checks that the value of min-zoom is number">
        <description>
          <test_script_entry>/opt/webapi-deviceadaptation-css3-tests/cssdeviceadaptation/min-zoom_number.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="WebAPI/Responsive Design/CSS Device Adaptation" execution_type="manual" id="min-zoom_percentage" purpose="Test checks that the value of min-zoom is percentage">
        <description>
          <test_script_entry>/opt/webapi-deviceadaptation-css3-tests/cssdeviceadaptation/min-zoom_percentage.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="WebAPI/Responsive Design/CSS Device Adaptation" execution_type="manual" id="height_one_value" purpose="Test checks that the descriptor of height has one value that will be set both min-height and max-height">
        <description>
          <test_script_entry>/opt/webapi-deviceadaptation-css3-tests/cssdeviceadaptation/height_one_value.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="WebAPI/Responsive Design/CSS Device Adaptation" execution_type="manual" id="height_two_value" purpose="Test checks that the descriptor of height has two value that will be set min-height to the first and max-height to the second">
        <description>
          <test_script_entry>/opt/webapi-deviceadaptation-css3-tests/cssdeviceadaptation/height_two_value.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="WebAPI/Responsive Design/CSS Device Adaptation" execution_type="manual" id="width_one_value" purpose="Test checks that the descriptor of height has one value that will be set both min-height and max-height">
        <description>
          <test_script_entry>/opt/webapi-deviceadaptation-css3-tests/cssdeviceadaptation/width_one_value.html</test_script_entry>
        </description>
      </testcase>
    </set>
  </suite>
</test_definition>
