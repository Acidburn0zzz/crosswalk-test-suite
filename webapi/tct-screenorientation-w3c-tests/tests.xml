<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="./testcase.xsl"?>
<test_definition>
  <suite category="W3C/HTML5 APIs" launcher="xwalk" name="tct-screenorientation-w3c-tests">
    <set name="ScreenOrientation">
      <testcase component="WebAPI/Device/The Screen Orientation API" execution_type="auto" id="Screen_exists" purpose="Check if the window.screen exists">
        <description>
          <test_script_entry>/opt/tct-screenorientation-w3c-tests/screenorientation/Screen_attributes_tests.html?total_num=10&amp;locator_key=id&amp;value=1</test_script_entry>
        </description>
      </testcase>
      <testcase component="WebAPI/Device/The Screen Orientation API" execution_type="auto" id="Screen_lockOrientation_exists" purpose="Check if the method lockOrientation of interface exists">
        <description>
          <test_script_entry>/opt/tct-screenorientation-w3c-tests/screenorientation/Screen_attributes_tests.html?total_num=10&amp;locator_key=id&amp;value=2</test_script_entry>
        </description>
      </testcase>
      <testcase component="WebAPI/Device/The Screen Orientation API" execution_type="auto" id="Screen_lockOrientation_return_type" purpose="Check if the method lockOrientation of interface Screen return type of boolean">
        <description>
          <test_script_entry>/opt/tct-screenorientation-w3c-tests/screenorientation/Screen_attributes_tests.html?total_num=10&amp;locator_key=id&amp;value=3</test_script_entry>
        </description>
      </testcase>
      <testcase component="WebAPI/Device/The Screen Orientation API" execution_type="auto" id="Screen_onorientationchange_exists" purpose="Check if the attribute onorientationchange of interface exists">
        <description>
          <test_script_entry>/opt/tct-screenorientation-w3c-tests/screenorientation/Screen_attributes_tests.html?total_num=10&amp;locator_key=id&amp;value=4</test_script_entry>
        </description>
      </testcase>
      <testcase component="WebAPI/Device/The Screen Orientation API" execution_type="auto" id="Screen_onorientationchange_return_type" purpose="Check if the attribute onorientationchange of interface Screen return type of function">
        <description>
          <test_script_entry>/opt/tct-screenorientation-w3c-tests/screenorientation/Screen_attributes_tests.html?total_num=10&amp;locator_key=id&amp;value=5</test_script_entry>
        </description>
      </testcase>
      <testcase component="WebAPI/Device/The Screen Orientation API" execution_type="auto" id="Screen_orientation_exists" purpose="Check if the attribute orientation of interface exists">
        <description>
          <test_script_entry>/opt/tct-screenorientation-w3c-tests/screenorientation/Screen_attributes_tests.html?total_num=10&amp;locator_key=id&amp;value=6</test_script_entry>
        </description>
      </testcase>
      <testcase component="WebAPI/Device/The Screen Orientation API" execution_type="auto" id="Screen_orientation_return_type" purpose="Check if the attribute orientation of interface Screen return type of string">
        <description>
          <test_script_entry>/opt/tct-screenorientation-w3c-tests/screenorientation/Screen_attributes_tests.html?total_num=10&amp;locator_key=id&amp;value=7</test_script_entry>
        </description>
      </testcase>
      <testcase component="WebAPI/Device/The Screen Orientation API" execution_type="auto" id="Screen_orientation_readonly" purpose="Check if the attribute orientation of interface Screen is readonly">
        <description>
          <test_script_entry>/opt/tct-screenorientation-w3c-tests/screenorientation/Screen_attributes_tests.html?total_num=10&amp;locator_key=id&amp;value=8</test_script_entry>
        </description>
      </testcase>
      <testcase component="WebAPI/Device/The Screen Orientation API" execution_type="auto" id="Screen_orientation_in_currentorientation" purpose="Check if the attribute orientation value of interface Screen must in current orientation">
        <description>
          <test_script_entry>/opt/tct-screenorientation-w3c-tests/screenorientation/Screen_attributes_tests.html?total_num=10&amp;locator_key=id&amp;value=9</test_script_entry>
        </description>
      </testcase>
      <testcase component="WebAPI/Device/The Screen Orientation API" execution_type="auto" id="Screen_unlockOrientation_exists" purpose="Check if the method unlockOrientation of interface exists">
        <description>
          <test_script_entry>/opt/tct-screenorientation-w3c-tests/screenorientation/Screen_attributes_tests.html?total_num=10&amp;locator_key=id&amp;value=10</test_script_entry>
        </description>
      </testcase>
      <testcase component="WebAPI/Device/The Screen Orientation API" execution_type="auto" id="Screen_lockOrientation_orientation_invalid" purpose="Test checks that the method lockOrientation return false, when parameter is not in allowed orientations">
        <description>
          <test_script_entry>/opt/tct-screenorientation-w3c-tests/screenorientation/Screen_lockOrientation_orientation_invalid.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="WebAPI/Device/The Screen Orientation API" execution_type="auto" id="Screen_lockOrientation_orientation_null" purpose="Test checks that the method lockOrientation return false, when parameter with null">
        <description>
          <test_script_entry>/opt/tct-screenorientation-w3c-tests/screenorientation/Screen_lockOrientation_orientation_null.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="WebAPI/Device/The Screen Orientation API" execution_type="auto" id="Screen_lockOrientation_orientation_landspace" purpose="Test checks that lockOrientation('landspace') locks screen as landscape">
        <description>
          <test_script_entry>/opt/tct-screenorientation-w3c-tests/screenorientation/Screen_lockOrientation_orientation_landspace.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="WebAPI/Device/The Screen Orientation API" execution_type="auto" id="Screen_unlockOrientation_base" purpose="Check if the method unlockOrientation can disable the current lock">
        <description>
          <test_script_entry>/opt/tct-screenorientation-w3c-tests/screenorientation/Screen_unlockOrientation_base.html</test_script_entry>
        </description>
      </testcase>
    </set>
  </suite>
</test_definition>
