{
  "name": "splashscreen_launchscreen_bmp_tests",
  "version": "0.1",
  "app": {
    "launch":{
            "local_path":"index.html"
        }
  },
  "icons": { "117": "icon.png"},
  "launch-screen": {
    "ready-when": "first-paint | user-interactive | complete | custom",
    "portrait | landscape": {
      "background-color": "#ff0000",
      "background-image": "bg.png 1x, bg2.png 2x",
      "image": "loading.bmp 1x, loading2.bmp 2x",
      "image-border": "30px round"
    }
  }
}
