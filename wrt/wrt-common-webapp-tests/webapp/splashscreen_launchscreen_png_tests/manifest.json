{
  "name": "splashscreen_launchscreen_png_tests",
  "version": "0.1",
  "app": {
    "launch":{
            "local_path":"index.html"
        }
  },
  "icons": { "117": "icon.png"},
  "launch-screen": {
    "ready-when": "first-paint | user-interactive | complete | custom",
    "portrait | landscape": {
      "background-color": "#0000ff",
      "background-image": "bg.jpg 1x, bg2.jpg 2x",
      "image": "loading.png 1x, loading2.png 2x",
      "image-border": "30px 40px repeat"
    }
  }
}
