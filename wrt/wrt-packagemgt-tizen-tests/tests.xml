<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="./testcase.xsl"?>
<test_definition>
  <suite category="Package_Management_on_Tizen" launcher="xwalk" name="wrt-packagemgt-tizen-tests">
    <set name="packagemgt">
      <testcase component="Package Management On Tizen" execution_type="manual" id="Crosswalk_WebApp_Uninstall" purpose="Validate if Crosswalk support web apps could be uninstalled successfully">
        <description>
          <pre_condition>
            1.Make sure there some webapps are available
          </pre_condition>
          <test_script_entry>/opt/wrt-packagemgt-tizen-tests/packagemgt/Crosswalk_WebApp_Uninstall.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Package Management On Tizen" execution_type="manual" id="Crosswalk_Multiple_Apps_Launch" purpose="Validate if Crosswalk support multiple web apps could be launched successfully">
        <description>
          <pre_condition>
            1.Make sure Crosswalk application is launched
            2.Make sure multiple webapps are available
          </pre_condition>
          <test_script_entry>/opt/wrt-packagemgt-tizen-tests/packagemgt/Crosswalk_Multiple_Apps_Launch.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Package Management On Tizen" execution_type="manual" id="Crosswalk_Wiget_Install" purpose="Validate if Crosswalk support to install Widget">
        <description>
          <pre_condition>
            1.Make sure Crosswalk is installed;
            2.Make sure the wgt tct-2dtransforms-css3-tests.wgt available.
          </pre_condition>
          <test_script_entry>/opt/wrt-packagemgt-tizen-tests/packagemgt/Crosswalk_Wiget_Install.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Package Management On Tizen" execution_type="manual" id="Crosswalk_Wiget_Launch" purpose="Validate if Crosswalk support to launch the installed Widget">
        <description>
          <pre_condition>
            1.Make sure Crosswalk is installed;
            2.Make sure the wgt tct-2dtransforms-css3-tests.wgt is installed.
          </pre_condition>
          <test_script_entry>/opt/wrt-packagemgt-tizen-tests/packagemgt/Crosswalk_Wiget_Launch.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Package Management On Tizen" execution_type="manual" id="Crosswalk_Wiget_Uninstall" purpose="Validate if Crosswalk support to Uninstall the installed Widget">
        <description>
          <pre_condition>
            1.Make sure Crosswalk is installed;
            2.Make sure the wgt tct-2dtransforms-css3-tests.wgt is installed.
          </pre_condition>
          <test_script_entry>/opt/wrt-packagemgt-tizen-tests/packagemgt/Crosswalk_Wiget_Uninstall.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Package Management On Tizen" execution_type="manual" id="Crosswalk_Remote_Debugging_Connection" purpose="Validate Crosswalk could debug by remote host">
        <description>
          <pre_condition>
            1.Make sure Crosswalk is installed.
          </pre_condition>
          <test_script_entry>/opt/wrt-packagemgt-tizen-tests/packagemgt/Crosswalk_Remote_Debugging_Connection.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Package Management On Tizen" execution_type="manual" id="Crosswalk_Remote_Debugging_CheckInfo" purpose="Validate Crosswalk viewed webpage could be modified by remote debug">
        <description>
          <pre_condition>
            1.Make sure Crosswalk lanunched by remote debug mode.
          </pre_condition>
          <test_script_entry>/opt/wrt-packagemgt-tizen-tests/packagemgt/Crosswalk_Remote_Debugging_CheckInfo.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Package Management On Tizen" execution_type="manual" id="Crosswalk_Remote_Debugging_Modification" purpose="Validate Crosswalk viewed webpage could be modified by remote debug">
        <description>
          <pre_condition>
            1.Make sure Crosswalk lanunched by remote debug mode.
          </pre_condition>
          <test_script_entry>/opt/wrt-packagemgt-tizen-tests/packagemgt/Crosswalk_Remote_Debugging_Modification.html</test_script_entry>
        </description>
      </testcase>
